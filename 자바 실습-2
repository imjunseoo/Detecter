import java.util.*; 

public class main {

	public static void main(String[] args) 
	{		
		final int targetnum = 1000;
		Scanner input = new Scanner(System.in);		
		System.out.print("실적을 입력하십시오 :");
		int number = input.nextInt();
		int bonus;
		String result;
		
		if(number >= targetnum) 
		{
			result = "실적 달성";
			bonus = (number - targetnum)/10;
		}
		else 
		{
			result = "실적 미달성";
			bonus = 0;
		}		
		System.out.println(result + "\n" + "보너스 :" + bonus);
	}
}

